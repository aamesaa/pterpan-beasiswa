.textbox { 
	border: 1px solid #848484; 
	-webkit-border-radius: 5px; 
	-moz-border-radius: 5px; 
	border-radius: 5px; 
	outline:0; 
	height:25px; 
	padding-left:10px; 
	padding-right:10px; 
  } 
.textarea { 
	border: 1px solid #848484; 
	-webkit-border-radius: 5px; 
	-moz-border-radius: 5px; 
	border-radius: 5px; 
	outline:0; 
	padding-left:10px; 
	padding-right:10px; 
  } 
 .box {
	border-radius: 5px; 
	width:100%;
	background-color:#ffffff;
	font-size:15px;
	margin-top:10px;
	border: 0px solid #848484; 
	float:left;
	position:relative;
 }
 .left {
	font-family:"Helvetica";
	float: left;
	width: 32%;
	margin-right:5px;
	padding: 5px 5px 5px 5px;
 }
 .center {
	border: 0px solid #848484; 
	float: left;
	width: 34%;
	/*margin-left:5px;*/
	margin-right:5px;
	padding: 5px 5px 5px 5px;
	/*display: inline;*/
 }
 .right {
	border: 0px solid #848484; 
	float: left;
	width: 31%;
	padding: 5px 5px 5px 5px;

	/*display: inline;*/
 }

